package colToy;

/**
 *
 * @author  Bambang Hariyanto,Ir.MT
 */
public class SLLNode {
    protected Object o;
    protected SLLNode next;
    
    /** Creates a new instance of SLLNode */
    public SLLNode (Object o, SLLNode next){
        this.o = o;
        this.next = next;
    }
    
    public Object getObject(){
        return o;
    }
    
    public String toString(){
        return ""+o;
    }
}
