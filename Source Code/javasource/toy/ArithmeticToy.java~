package toy;

/**
 *
 * @author  Bambang Hariyanto,Ir.MT
 */
class ArithmeticApp {  
    public static void main (String args[]) {    
        int x = 17, y = 5;    
        
        System.out.println("x = " + x);    
        System.out.println("y = " + y);    
        System.out.println("x + y = " + (x + y));    
        System.out.println("x - y = " + (x - y));    
        System.out.println("x * y = " + (x * y));    
        System.out.println("x / y = " + (x / y));    
        System.out.println("x % y = " + (x % y));  
    }
}