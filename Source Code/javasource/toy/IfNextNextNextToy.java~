public class IfNextNextNextToy {
    public static void main(String args[]){
        int anInt = 0;
        if (anInt == 0) 
            System.out.println("Variabel anInt bernilai Nol");
        else 
            System.out.println("Variabel anInt tidak bernilai Nol");
    }
}